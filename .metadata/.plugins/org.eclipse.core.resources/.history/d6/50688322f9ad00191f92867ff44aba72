package pack;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        
     String var = "sample";
     
     char char1[]=var.toCharArray();
    
    for(int i=var.length();i<var.length();i--)
    {
    	System.out.print(char1[i]);
    }
     
    
    	
    	
    	
    	
    }
}
